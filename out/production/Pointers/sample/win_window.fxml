<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #34495e;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.WinWindowController">
   <children>
      <Label layoutX="24.0" layoutY="26.0" text="You win!" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="31.0" />
         </font>
      </Label>
      <Button fx:id="PlayAgainButton" layoutX="53.0" layoutY="115.0" mnemonicParsing="false" style="-fx-background-color: #ecf0f1;" text="Play again">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
